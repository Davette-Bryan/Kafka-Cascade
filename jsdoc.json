{
  "name": "Kafka-Cascade Documentation",
  "opts": {
    "readme": "./docs/docs.md",
    "template": "node_modules/better-docs"
  },
  "tags": {
    "allowUnknownTags": ["optional"]
  },
  "plugins": [
      "node_modules/better-docs/typescript"
  ],
  "source": {
      "includePattern": "\\.(jsx|js|ts|tsx)$"
  },
  "templates": {
    "name": "Kafka-Cascade Documentation",
    "better-docs": {
      "name": "Kafka-Cascade Documentation",
      "title": "Kafka-Cascade Documentation",
      "logo": "favIconLarger.png",
      "css": "style.css",
      "hideGenerator": false,
      "navLinks": [
        {

          "label": "Github",
          "href": "https://github.com/oslabs-beta/Kafka-Cascade"
        },
        {
          "label": "Home",
          "href": "/"
        }
      ]
    }
  }
}